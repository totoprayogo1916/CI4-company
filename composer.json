{
	"name": "totoprayogo1916/ci4-company",
	"description": "Company Profile Website Full Features with CI4",
	"license": "MIT",
	"type": "project",
	"homepage": "https://github.com/totoprayogo1916/CI4-company",
	"require": {
		"php": "^7.3 || ^8.0",
		"codeigniter4/framework": "^4"
	},
	"require-dev": {
		"fakerphp/faker": "^1.9",
		"mikey179/vfsstream": "^1.6"
	},
	"suggest": {
		"ext-fileinfo": "Improves mime type detection for files"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app",
			"Config\\": "app/Config"
		},
		"exclude-from-classmap": [
			"**/Database/Migrations/**"
		]
	}
}
